(CaminhoArquivo_GitHub) =>
let
	  // Importando dados de arquivos salvo em repositorio publico do GitHub.
    // Arquivo de Excel salvo com a extens√£o: CSV (separado por ";" )
    Fonte = Csv.Document(
        Web.Contents(
            CaminhoArquivo_GitHub
        ),
        [
            Delimiter=";", 
            Columns=3, 
            Encoding=65001, 
            QuoteStyle=QuoteStyle.None
        ]
    )    
in
    Fonte
