let

    /*
    15/07/2020 => Joviano Silveira
    Tabela de Caracteres extraida de https://www.w3schools.com/charsets/ref_html_entities_4.asp
    */
    
    Fonte = Table.FromRows(
        Json.Document(
            Binary.Decompress(
                Binary.FromText(
                    "", 
                    BinaryEncoding.Base64
                ), 
                Compression.Deflate
            )
        ), 
        let 
            _t = ((type nullable text) meta [Serialized.Text = true]) in type table [#"Coluna 1" = _t, #"Coluna 2" = _t, #"Coluna 3" = _t, #"Coluna 4" = _t, #"Coluna 5" = _t]),
    
    CabecalhosPromovidos = Table.PromoteHeaders(Fonte, [PromoteAllScalars=true]),
    
    BufferTable = Table.Buffer(CabecalhosPromovidos)

in
    
    BufferTable
